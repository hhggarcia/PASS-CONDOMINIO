@model IEnumerable<Prueba.Models.PuestoE>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}

<p>
    <a asp-action="Create" class="btn btn-light">Crear Nuevo</a>
    <a asp-action="Index" asp-controller="Estacionamientos" class="btn btn-light">Volver</a>

</p>

<div class="row">
    <div class="col-lg-12">
        <!-- Default Card Example -->
        <div class="card shadow mb-4 bg-content-bg">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">
                    Puestos de estacionamientos
                </h6>
            </div>
            <div class="card-body">
                <div class="form form-horizontal">
                    <div class="form-body">
                        <div class="row">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Codigo)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.Alicuota)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.IdEstacionamientoNavigation)
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.IdPropiedadNavigation)
                                        </th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Codigo)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Alicuota)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.IdEstacionamientoNavigation.IdEstacionamiento)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.IdPropiedadNavigation.IdPropiedad)
                                            </td>
                                            <td>
                                                <a asp-action="Edit" asp-route-id="@item.IdPuestoE">Editar</a> |
                                                <a asp-action="Details" asp-route-id="@item.IdPuestoE">Detalles</a> |
                                                <a asp-action="Delete" asp-route-id="@item.IdPuestoE">Eliminar</a>
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
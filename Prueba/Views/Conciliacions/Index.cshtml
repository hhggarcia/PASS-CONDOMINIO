@model IEnumerable<Prueba.Models.Conciliacion>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";

}

<div class="container-fluid">
    <!-- Page Heading -->
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <a class="btn btn-light mb-3"
           asp-action="Conciliacion">
            Conciliar
        </a>
    </div>

    <!-- Content Row -->
    <div class="row">
        <div class="col-lg-12 mb-4">
            <!-- DataTales Example -->
            <div class="card shadow mb-4 bg-content-bg">
                <div class="card-header py-3">
                    <h6 class="m-0 font-weight-bold text-primary">
                        Conciliaciones
                    </h6>

                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.FechaEmision)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.SaldoInicial)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.SaldoFinal)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Actual)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Activo)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.FechaInicio)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.FechaFin)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.IdCodCuentaNavigation)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.IdCondominioNavigation)
                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.FechaEmision)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.SaldoInicial)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.SaldoFinal)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Actual)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Activo)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.FechaInicio)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.FechaFin)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.IdCodCuentaNavigation.IdCodCuenta)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.IdCondominioNavigation.IdCondominio)
                                        </td>
                                        <td>
                                            <a asp-action="Edit" asp-route-id="@item.IdConciliacion">Editar</a> |
                                            <a asp-action="Details" asp-route-id="@item.IdConciliacion">Detalles</a> |
                                            <a asp-action="Delete" asp-route-id="@item.IdConciliacion">Eliminar</a>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



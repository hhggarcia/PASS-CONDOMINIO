@model Prueba.Models.Conciliacion

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}

<div class="container-fluid">
    <div class="row justify-content-center">
        <div class="col-lg-6">

            <div class="card shadow mb-4 bg-content-bg">
                <div class="card-header py-3">
                    <h6 class="m-0 font-weight-bold text-primary">
                        Conciliaciones
                    </h6>
                </div>
                <div class="card-body">
                    <div class="form form-horizontal">
                        <div class="form-body">
                            <dl class="row">
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.FechaEmision)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.FechaEmision)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.SaldoInicial)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.SaldoInicial)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.SaldoFinal)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.SaldoFinal)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.Actual)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Actual)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.Activo)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Activo)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.FechaInicio)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.FechaInicio)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.FechaFin)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.FechaFin)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.IdCodCuentaNavigation)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.IdCodCuentaNavigation.IdCodCuenta)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.IdCondominioNavigation)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.IdCondominioNavigation.IdCondominio)
                                </dd>
                            </dl>
                            <div class="col-md-12">
                                <a asp-action="Edit" asp-route-id="@Model?.IdConciliacion" class="btn btn-primary">Editar</a>
                                <a asp-action="Index" class="btn btn-secondary">Volver</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
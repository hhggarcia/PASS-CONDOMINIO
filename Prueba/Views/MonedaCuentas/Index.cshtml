@model IEnumerable<Prueba.Models.MonedaCuenta>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RecibePagos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCodCuentaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdMonedaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RecibePagos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCodCuentaNavigation.IdCodCuenta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdMonedaNavigation.IdMoneda)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

@model IEnumerable<Prueba.Models.PropiedadesGrupo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Alicuota)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdGrupoGastoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPropiedadNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Alicuota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdGrupoGastoNavigation.IdGrupoGasto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPropiedadNavigation.IdPropiedad)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPropiedadGrupo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdPropiedadGrupo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdPropiedadGrupo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

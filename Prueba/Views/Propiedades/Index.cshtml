@model IEnumerable<Prueba.Models.Propiedad>

@{
    ViewData["Title"] = "Index";
    Layout = "_LayoutAdministrador.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Codigo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dimensiones)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Alicuota)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Solvencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Saldo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Deuda)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MontoIntereses)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCondominioNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUsuarioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Codigo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dimensiones)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Alicuota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Solvencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Saldo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Deuda)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MontoIntereses)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCondominioNavigation.IdCondominio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUsuarioNavigation.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPropiedad">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdPropiedad">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdPropiedad">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
